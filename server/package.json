{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "tsc",
    "postinstall": "npm run build",
    "start": "node dist/index.js"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.11",
    "@types/ws": "^7.2.5",
    "husky": "^4.2.5",
    "lint-staged": ">=10",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "unique-names-generator": "^4.2.0",
    "uuidv4": "^6.1.0",
    "ws": "^7.4.6"
  },
  "engines": {
    "node": "<=12.14.0"
  }
}
